<!DOCTYPE html>
<html>
    <head>
        <title>Home Page</title>
     

    </head>
    <body>
        <h2>The list of users are</h2>

            <% for(datas of data){ %>
            <div><h3 class="post"><%= datas.id %></h3>
                <h3 class="user">@<%= datas.name %></h3>
                <h3><%= datas.email %></h3>

                <a href="/user/<%= datas.id %>/edit">Edit</a>
                 <form method = "post" action="/user/<%=datas.id %>?_method=DELETE">
                    <button>Delete</button>
                </form>
               
            </div>    
            <hr>
        <%} %>





    </body>
</html>